generator client {
  provider = "prisma-client-js"
}

generator nexusPrisma {
  provider = "nexus-prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id                  Int       @id @default(autoincrement())
  qruid               String    @unique
  has_deposit         Boolean
  brand_id            String
  date_of_activation  DateTime  @default(now())
  deposit_amount      Float
  customer_id         String?
  merchant_id         String?
  passport_definition String?
  date_of_return      DateTime?
}
